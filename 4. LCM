// 4. Write a program to find the LCM of any number of numbers.


//function to find gcd of a and b
fun gcd(a: Int, b: Int): Int {  
	if (b == 0)
		return a;
	return gcd(b, a % b);
}

// Returns LCM of array elements
fun findLCM(ar: ArrayList<Int>): Int 
{
	// Initialize result
	var ans = ar.get(0);

	// ans contains LCM of arr[0], ..arr[i]
	// after ith iteration,
	for (i in ar)
		ans = (((i * ans)) /
				(gcd(i, ans)));

	return ans;
}

fun main (){
    var arrayint = ArrayList<Int>()  //initializing an array
    var x = 0
    
    //taking input for the size of the array
    val abc = Integer.valueOf(readLine())  
    
    //taking the input for the array
    while( x < abc)
    {
        var t = readLine()!!.toInt()
        arrayint.add(t)
        x++
    }
    
    //passing the array to findLCM function to calculate the LCM of the numbers in the array.
    var res = findLCM(arrayint)  
    
    println("LCM is: $res")
}
